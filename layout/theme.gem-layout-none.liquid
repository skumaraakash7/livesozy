<head>
  <!-- better-open-graph-tags-head-begin -->
  {% if shop.metafields.bopengraphtags.enabled == "true" %}
  {% capture snippet_content %}{% include 'better-open-graph-tags-open-graph-tags'  %}{% endcapture %}
  {% unless snippet_content contains "Liquid error" %}
  {{ snippet_content }}
  {% endunless %}
  {% endif %}
  <!-- better-open-graph-tags-head-end -->


  <meta charset="utf-8" />

  {% include 'google-fonts' %}
  {% include 'social-media-meta-tags' %}

  <!-- SEO
================================================== -->

  <title>
    {% if template == 'index' %}{% if page_title contains 'Welcome' %}{{ shop.name }} &ndash; Welcome{% else %}{{ page_title }}{% endif %}{% elsif template == '404' %}Page Not Found{% elsif page_title contains shop.name %}{{ page_title }}{% else %}{{ page_title }} &ndash; {{ shop.name }}{% endif %}
  </title>
  {% if page_description %}
  <meta name="description" content="{{ page_description }}" />
  {% endif %}
  <link rel="canonical" href="{{ canonical_url }}" />

  <!-- CSS
================================================== -->

  {{ 'stylesheet.css' | asset_url | stylesheet_tag }}
  {{ 'queries.css' | asset_url | stylesheet_tag }}

  <!-- JS
================================================== -->

  <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
  {{ 'theme.js' | asset_url | script_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}  
  {% if template  == 'index'  %}{{ 'instafeed.js' | asset_url | script_tag }}{% endif %}

  <meta name="viewport" content="width=device-width, initial-scale=1">
  
  {% if settings.favicon %}
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png" />
  {% endif %}
  {% if template contains 'customers' %}
  {{ "shopify_common.js" | shopify_asset_url | script_tag }}
  {{ "customer_area.js"  | shopify_asset_url | script_tag }}
  {% endif %}
  {{ content_for_header }}
  <link rel="next" href="/next"/>
  <link rel="prev" href="/prev"/>
  
<!--Gem_Page_Header_Script-->
{% include 'gem-app-header-scripts' %}
<!--End_Gem_Page_Header_Script-->

  

  {% render 'shogun-head' %}
  <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/zsiPgMo9N2G4ba7_kt6Jrg" async></script>
</head>

<body>
